		"@types/node": "^16.11.49",
		"@typescript-eslint/eslint-plugin": "^5.33.1",
		"@typescript-eslint/parser": "^5.33.1",
		"better-sqlite3": "^7.6.2",
		"chai": "^4.3.6",
		"chai-as-promised": "^7.1.1",
		"eslint": "^8.22.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-mocha": "^9.0.0",
